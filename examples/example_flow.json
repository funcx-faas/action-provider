{
  "Comment": "A flow to execute tasks via FuncX Action Provider",
  "StartAt": "RunFuncX",
  "States": {
    "RunFuncX": {
      "Type": "Action",
      "ActionUrl": "automate-test.funcx.org",
      "ActionScope": "https://auth.globus.org/scopes/b3db7e59-a6f1-4947-95c2-59d6b7a70f8c/action_all",
      "Parameters": {
        "tasks.$": "$.funcx_tasks",
        "task_count.$": "$.task_count"
      },
      "Next": "EndFuncX"
    },
    "EndFuncX": {
      "Type": "Pass",
      "End": true
    }
  }
}