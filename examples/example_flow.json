{
  "Comment": "A flow to execute tasks via FuncX Action Provider",
  "StartAt": "RunFuncX",
  "States": {
    "RunFuncX": {
      "Type": "Action",
      "ActionUrl": "https://automate-test.funcx.org/fxap",
      "ActionScope": "https://auth.globus.org/scopes/b3db7e59-a6f1-4947-95c2-59d6b7a70f8c/action_all",
      "Parameters": {
        "endpoint.=": "getattr('endpoint')",
        "function.=": "getattr('function')",
        "args.=": "getattr('args')",
        "kwargs.=": "getattr('kwargs')",
        "endpoint_2.=": "getattr('endpoint_2')",
        "function_2.=": "getattr('function_2')",
        "args_2.=": "getattr('args_2')",
        "kwargs_2.=": "getattr('kwargs_2')",
        "tasks.=": "getattr('tasks')"
      },
      "ResultPath": "$.RunResult",
      "Next": "EndFuncX"
    },
    "EndFuncX": {
      "Type": "Pass",
      "End": true
    }
  }
}
